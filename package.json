{
  "name": "metabuidl-2021-novum-depositum",
  "version": "1.0.0-beta.0",
  "repository": "git@github.com:InfinityHackathon/metabuidl-2021-novum-depositum.git",
  "license": "UNLICENSED",
  "browserslist": [
    "defaults",
    "Firefox ESR",
    "not IE 11"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_public": "ng serve --disable-host-check",
    "build": "ng build --base-href=/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy_contract": "npx ts-node src/contract/deploy",
    "deploy_web": "ng deploy --base-href=/",
    "setup_state": "bash util/setup-stage.sh",
    "setup-env": "yarn setup-env-1 && yarn setup-env-2",
    "setup-env-1": "npx ts-node util/setup-env-s1",
    "setup-env-2": "npx ts-node util/setup-env-s2",
    "setup-near": "npx ts-node util/setup",
    "setup-testnet": "npx ts-node util/setup-testnet",
    "setup": "yarn up && yarn setup-near",
    "up": "yarn down && docker-compose up --detach",
    "down": "docker-compose down --volumes"
  },
  "dependencies": {
    "@4ire-labs/near-sdk": "1.0.0-beta.8",
    "@angular/animations": "^12.2.5",
    "@angular/cdk": "^12.2.5",
    "@angular/common": "^12.2.5",
    "@angular/compiler": "^12.2.5",
    "@angular/core": "^12.2.5",
    "@angular/forms": "^12.2.5",
    "@angular/material": "^12.2.5",
    "@angular/platform-browser": "^12.2.5",
    "@angular/platform-browser-dynamic": "^12.2.5",
    "@angular/router": "^12.2.5",
    "@angular/service-worker": "^12.2.5",
    "near-api-js": "^0.42.0",
    "rxjs": "^7.3.0",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.2",
    "@angular/cli": "^12.2.5",
    "@angular/compiler-cli": "~12.1.2",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "1.0.0-rc.2",
    "dotenv": "^10.0.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "near-cli": "^2.1.1",
    "ts-node": "^10.2.1",
    "typescript": "<4.4.0"
  }
}
